query getUserById($id: String!) {
  getUserById(id: $id) {
    userName
    firstname
    lastname
    password
    chatrooms {
      title
    }
    avatar
    isConnected
    email
    birthDate
    hobbies {
      title
      category
    }
  }
}



variables: {
    id: "610178dbc12889006a058ff3"
}